{
    "name": "react-vite-js",
    "private": true,
    "version": "0.0.1",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist",
        "build": "vite build --mode production",
        "format": "prettier --ignore-path .prettierignore --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
        "lint": "eslint --ignore-path .eslintignore . --ext ts --ext tsx --ext js --ext jsx",
        "lint:fix": "yarn format & next lint --dir src",
        "clean-cache": "rimraf .next",
        "clean": "rimraf node_modules .next"
    },
    "dependencies": {
        "@tanstack/react-query": "^5.51.11",
        "classnames": "^2.5.1",
        "date-fns": "^3.6.0",
        "framer-motion": "^11.0.13",
        "highcharts": "^11.4.3",
        "highcharts-react-official": "^3.2.1",
        "http-proxy-middleware": "^3.0.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-icons": "^5.0.1",
        "react-router-dom": "^6.24.1"
    },
    "devDependencies": {
        "@types/react": "^18.2.64",
        "@types/react-dom": "^18.2.21",
        "@vitejs/plugin-react-swc": "^3.7.0",
        "autoprefixer": "^10.4.18",
        "eslint": "^8.57.0",
        "eslint-plugin-react": "^7.34.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-react-refresh": "^0.4.5",
        "gh-pages": "^6.1.1",
        "postcss": "^8.4.35",
        "serve": "^14.2.3",
        "tailwindcss": "^3.4.1",
        "vite": "^5.1.6"
    }
}
